#include <stdio.h>

int main()
{
    int n;
    printf("enter the size");
    scanf("%d",&n);
    int a[n],i,j,flag=0;
    for(i=0;i<n;i++)
    scanf("%d",&a[i]);
    printf("prime number are\n");
    for(i=0;i<n;i++)
    {
        flag=0;
        for(j=2;j<=a[i]/2;j++)
        {
            if(a[1%j==0])
            {
                flag=1;
                break;
            }
            for(j=1;j<n;j++)
            if(flag==0)
            {
                printf("%d",&a[i]);
                printf("\n");
            }
            return 0;
        }
    }
}


